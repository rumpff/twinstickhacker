    var vRes = shader_get_uniform(shader0,"vRes");
    var vTime = shader_get_uniform(shader0,"vTime");
    var vSwitch = shader_get_uniform(shader0,"vSwitch");
    var yOffset = shader_get_uniform(shader0,"yOffset");
    shader_set(shader0);
