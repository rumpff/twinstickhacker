    shader_set_uniform_f(vTime,counter);
    shader_set_uniform_f(vSwitch,vswitch);
    shader_set_uniform_f(yOffset,y_offset0);
    
    if(!surface_exists(global.mainSurface))
        global.mainSurface = surface_create(view_wport[0],view_hport[0]);
    
    surface_set_target(global.mainSurface);
    draw_clear_alpha(c_white,1);
    surface_reset_target();
    
    draw_surface_stretched(global.mainSurface,0,0,view_wport[0],view_hport[0]);
    
